Prog.   Program     ::= [Declaration] ;

Func.   Declaration ::= Ident [Arg] ":=" Exp ;

EArg.   Arg         ::= Exp ;

EVar.   Exp         ::= Ident ;
EInt.   Exp         ::= Integer ;
EApp.   Exp         ::= Exp Exp ;
EAbs.   Exp         ::= "\\" Ident "->" Exp ;

separator  Arg "," ;
terminator Declaration ";" ;
terminator Ident "" ;

comment "--" ;
comment "{-" "-}" ;

